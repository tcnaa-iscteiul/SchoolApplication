{
    "version": 2,
    "public": false,
    "builds": [
      {
        "src": "/Api/src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/Api/src/main.ts",
        "methods": [
          "GET",
          "POST",
          "PUT",
          "DELETE",
          "OPTIONS",
          "PATCH"
        ]
      }
    ]
  }
